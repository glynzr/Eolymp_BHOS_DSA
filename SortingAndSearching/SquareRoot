#include <iostream>
#include <cmath>
using namespace std;

double C;

bool can(double x) {
    return x * x + sqrt(x) <= C;
}

int main() {
    cin >> C;
    double L = 0, R = C;
    for (int i = 0; i < 100; i++) {
        double mid = (L + R) / 2;
        if (can(mid)) L = mid;
        else R = mid;
    }
    printf("%.6f\n", L);
    return 0;
}
