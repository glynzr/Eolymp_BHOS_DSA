#include <iostream>
#include <vector>

using namespace std;

int main() {
    int t;
    cin >> t;

    while (t--) {
        int n, s = 0;
        cin >> n;

        vector<int> a(n);
        for (int i = 0; i < n; i++) {
            cin >> a[i];
        }

        // bubble sort
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n - 1 - i; j++) {
                if (a[j] > a[j+1]) {
                    swap(a[j], a[j+1]);
                    s++;
                }
            }
        }

        cout << "Optimal train swapping takes " << s << " swaps." << endl;
    }

    return 0;
}
