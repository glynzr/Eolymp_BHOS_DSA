#include <iostream>
#include <vector>
using namespace std;

int main() {
    int n, M;
    cin >> n >> M;
    vector<int> cars(n);
    for (int i = 0; i < n; i++) {
        cin >> cars[i];
    }
    bool possible = true;
    for (int i = 0; i < n - 1; i++) {
        if (cars[i] > cars[i+1]) {
            int total_weight = cars[i] + cars[i+1];
            if (total_weight > M) {
                possible = false;
                break;
            }
       
            int temp = cars[i];
            cars[i] = cars[i+1];
            cars[i+1] = temp;
        }
    }
    if (possible) {
        cout << "Yes\n";
    } else {
        cout << "No\n";
    }
    return 0;
}
