#include <iostream>
using namespace std;

int binarySearch(int arr[], int l, int r, int x) {
    while (l <= r) {
        int m = l + (r - l) / 2;
        if (arr[m] == x)
            return m;
        if (arr[m] < x)
            l = m + 1;
        else
            r = m - 1;
    }
    return -1;
}

int main() {
    int n, q;
    cin >> n >> q;

    int arr[n];
    for (int i = 0; i < n; i++)
        cin >> arr[i];

    while (q--) {
        int x;
        cin >> x;

        int index = binarySearch(arr, 0, n - 1, x);
        if (index != -1)
            cout << "YES\n";
        else
            cout << "NO\n";
    }

    return 0;
}
