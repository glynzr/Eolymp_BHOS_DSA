#include <iostream>
using namespace std;

int binary_search(int arr[], int n, int x, bool firstOccurrence) {
    int lo = 0, hi = n - 1, result = -1;
    while (lo <= hi) {
        int mid = lo + (hi - lo) / 2;
        if (arr[mid] == x) {
            result = mid;
            if (firstOccurrence) hi = mid - 1; // search for first occurrence
            else lo = mid + 1; // search for last occurrence
        }
        else if (arr[mid] < x) lo = mid + 1;
        else hi = mid - 1;
    }
    return result;
}

int main() {
    int n, q;
    cin >> n >> q;
    int arr[n];
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }
    while (q--) {
        int x;
        cin >> x;
        int first = binary_search(arr, n, x, true);
        if (first == -1) {
            cout << 0 << endl;
        }
        else {
            int last = binary_search(arr, n, x, false);
            cout << last - first + 1 << endl;
        }
    }
    return 0;
}
